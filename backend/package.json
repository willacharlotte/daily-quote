{
  "name": "daily-quote-be",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "tsc && node ./dist/app.js",
    "start:dev": "npx nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "./src/"
    ],
    "ext": "*",
    "delay": 2000,
    "exec": "npx ts-node ./src/app.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.1438.0",
    "cors": "^2.8.5",
    "daily-quote": "file:..",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "zod": "^3.22.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1"
  }
}
